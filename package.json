{
  "name": "leningo",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-lint": "eslint \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "test-all": "yarn run check-lint && yarn run check-format && yarn run check-types && yarn run build",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint-eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\""
  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
